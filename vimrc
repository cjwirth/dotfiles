set nocompatible              " be iMproved
filetype off                  " Vundle required!

set rtp+=~/.vim/bundle/vundle/
call vundle#rc()

" let Vundle manage Vundle
" Vundle required! 
Bundle 'gmarik/vundle'

syntax on
filetype plugin indent on " Vundle Required! but also nice

:set tabstop=4
:set shiftwidth=4
:set expandtab

